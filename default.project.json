{
	"name": "launcher-battle",
	"tree": {
		"$className": "DataModel",

		"ReplicatedStorage": {
			"Common": {
				"$path": "src/shared"
			}
		},

		"ServerScriptService": {
			"Server": {
				"$path": "src/server"
			}
		},

		"StarterPlayer": {
			"$properties": {
				"CameraMode": "LockFirstPerson"
			},
			"StarterPlayerScripts": {
				"Client": {
					"$path": "src/client"
				}
			}
		},

		"Workspace": {
			"$className": "Workspace",
			"$ignoreUnknownInstances": true,
			"$path": "models",
			"$properties": {
				"FilteringEnabled": true
			}
		},
		"Lighting": {
			"$properties": {
				"Ambient": [0,0,0],
				"OutdoorAmbient": [0.5,0.5,0.5],
				"Brightness": 3,
				"GlobalShadows": true,
				"Outlines": false,
				"Technology": "Voxel"
			}
		},
		"SoundService": {
			"$properties": {
				"RespectFilteringEnabled": true
			}
		}
	}
}
